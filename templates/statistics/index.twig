{% extends "layouts/admin.twig" %}

{% block title %}UM | Statistics{% endblock %}

{% block content %}
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Statistics</h1>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="row">
                <form class="form form-horizontal" enctype="multipart/form-data" role="form" method="post" action="/um/users/import">
                    {% if error %}
                        <div class="alert alert-danger" role="alert">
                            <strong>Oh snap!</strong> {{ error }}
                        </div>
                    {% endif %}
                    <div class="row">
                        <table id="users" class="table-striped table">
                            <thead>
                            <tr>
                                <th>Acquire ID</th>
                                <th>Last Name</th>
                                <th>First Name</th>
                                <th>Middle Name</th>
                                <th>Department</th>
                                <th>Unit</th>
                                <th>Hire Date</th>
                                <th>Designation</th>
                                <th>Birth Date</th>
                                <th>Status</th>
                                <th>Actions</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </form>
            </div>
        </div>
    </div>
{% endblock %}